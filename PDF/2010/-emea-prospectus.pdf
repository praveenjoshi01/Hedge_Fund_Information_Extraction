}
}).done(function(data) {
if(data.length === 0){
data.push({label: "Sorry, no results found.", id: "", category: "noResults"});
}
response(data);
}).always(function() {
//$('.siteSearch .submitSearch').removeClass('loading');
});
},
close : function (event, ui) {
$(".ui-autocomplete").show();
}
});
}
var myResult = autoCompleteFreeformVariant();
