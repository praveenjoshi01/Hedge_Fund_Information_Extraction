<script type="text/javascript">
    $(document).ready(function() {
        
        // var tbApiToken = 'zzz';
        var tbFile = '/wp-content/themes/thornburg/data/disclosures.php'
        
        var tbDisclosures = {
            disData: '',
            setDisclosures: function() {
                var that = this;
                $('.js---disclosures').each(function( index ) {
                    var string  = $(this).attr('data-disclosures') || "";
                    // console.log('do:'+string); //test
                    var temp = '';
                    if (string.indexOf(',') > -1) { 
                        var disArray = string.split(',');
                        var i;
                        for (i = 0; i < disArray.length; i++) { 
                            var x = _.findIndex(that.disData, function(o) { return o.id == disArray[i]; });
                            // temp = temp + '<p data-did="'+that.disData[x].id+'">[' + that.disData[x].id + ']' + that.disData[x].html + '</p>'; //test
                            if (x >= 0) {
                                temp = temp + '<p data-did="'+that.disData[x].id+'">' + that.disData[x].html + '</p>';
                            }
                            
                        }
                    }
                    else {
                        //single
                        var x = _.findIndex(that.disData, function(o) { return o.id == string; });
                        // temp = temp + '<p data-did="'+that.disData[x].id+'">[' + that.disData[x].id + ']' + that.disData[x].html + '</p>'; //test
                        if (x >= 0) {
                            temp = temp + '<p data-did="'+that.disData[x].id+'">' + that.disData[x].html + '</p>';
                        }
                    }
                        
                    $(this).append(temp);

                });

                this.bindActions();
            },
            getData: function() {
                var that = this;

                var config = {
                    headers: {
                        // 'X-AUTH-TOKEN': tbApiToken,
                        'Accept': '*/*',
                        'Content-Type': 'application/json'
                    }
                };

                ///Prod
                axios.get(tbFile
                // that.query
                , config)
                .then(function (response) {
                    // console.log(response);
                    that.disData = response.data.data;
                    // // console.log(that.disData);
                    that.setDisclosures();
                })
                .catch(function (error) {
                    console.log(error);
                    that.bindActions();
                });
            },
            bindActions: function() {

                var toggled = 0;
                var height = 20;
                var starting_height = "7.5rem";
                var lastDisclosureId = '';

                //show more
                if ($('.page_disclosure_wrapper').length){
                    if ($('.page_disclosure_wrapper .disclosure').outerHeight() > 75) {
                        //show
                        // console.log('do showmore::'+$('.page_disclosure_wrapper .disclosure').outerHeight()); //test
                        $('.page_disclosure_wrapper .disclosure').css('height', starting_height);
                        
                    } else {
                        // console.log('hide show more::'+$('.page_disclosure_wrapper .disclosure').outerHeight()); //test
                        $('.page_disclosure_wrapper .disclosure_footer').hide();
                    }
                }

                $(".reveal_disclosure").on('click', function() {
                    // console.log(toggled);
                    // calculate the accumulated height of 'p' of disclosure div you clicked.
                    var obj = $(this).parent().next().children(); //find('p');
                    $.each(obj, function(){
                        height += $(this).outerHeight(true);
                    });
            
                    // In case there are more than one disclosure in one page. Each reveal_disclosure div should have a different id.
                    if($(this).attr('id') !== lastDisclosureId){
                        if($(this).text() == 'Show More') toggled = 0;
                        else toggled = 1;
                    }
            
                    if(toggled === 0){
                        toggled = 1;
                        $(this).parent().next().animate({"height":height + 25}, 400);
                        // console.log({"height":height});
                        $(this).text('Show Less');
                    } else {
                        toggled = 0;
                        $(this).parent().next().animate({"height":starting_height}, 400);
                        $(this).text('Show More');
                    }
                    //lastDisclosureId = $(this).attr('id');
                    // console.log("Last Disclosure Id: " + lastDisclosureId);
                    height = 20;
                });

                $(window).on('resizeEnd', function(){
                    if (toggled == 1) {
                        // recalc height and adjust
                        var newHeight = 45;
                        var obj = $(".reveal_disclosure").parent().next().children();
                        $.each(obj, function(){
                            newHeight += $(this).outerHeight(true);
                        });
                        $(".reveal_disclosure").parent().next().animate({"height":newHeight}, 400);
                    }
                });

            },
            init: function() {
                this.getData();
            }
        };
        tbDisclosures.init();



        //RCP JS
        //$('#rcp_login_form label[for="rcp_user_login"]').append(' or Email');


    });
</script>
